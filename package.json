{
	"name": "frostbitten-static-portfolio-generator",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"tsx watchData\" \"vite dev --host\"",
		"build": "tsx watchData build && tsx beforeBuild && vite build && tsx postBuild",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"refresh-token": "node getRefreshToken.js",
		"pull-gdrive": "tsx pullGDrive",
		"dev-host": "vite dev --host",
		"watch-data": "tsx watchData",
		"build-gdrive": "tsx pullGDrive && tsx watchData build && tsx beforeBuild && vite build && tsx postBuild"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/ffprobe": "^1.1.8",
		"@types/ffprobe-static": "^2.0.3",
		"@types/fs-extra": "^11.0.4",
		"@types/markdown-it": "^13.0.7",
		"@types/markdown-it-attrs": "^4.1.3",
		"@types/markdown-it-container": "^2.0.9",
		"@types/mime-types": "^2.1.4",
		"@types/node": "^20.11.5",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"sass": "^1.69.6",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@googleapis/drive": "^8.16.0",
		"chokidar": "^3.5.3",
		"concurrently": "^8.2.2",
		"crc-32": "^1.2.2",
		"dotenv": "^16.4.7",
		"extract-zip": "^2.0.1",
		"ffprobe": "^1.1.2",
		"ffprobe-static": "^3.1.0",
		"fs": "^0.0.1-security",
		"fs-extra": "^11.2.0",
		"googleapis": "^146.0.0",
		"markdown-it": "^14.0.0",
		"markdown-it-attrs": "^4.1.6",
		"markdown-it-container": "^4.0.0",
		"markdown-it-html5-embed": "^1.0.0",
		"mime-types": "^2.1.35",
		"open": "^10.1.0",
		"p-limit": "^5.0.0",
		"path": "^0.12.7",
		"server-destroy": "^1.0.1",
		"sharp": "^0.33.2",
		"svelte-inview": "^4.0.2",
		"tsx": "^4.19.3",
		"uuid": "^9.0.1",
		"webp-converter": "^2.3.3",
		"xss": "^1.0.14"
	}
}
